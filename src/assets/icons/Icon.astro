---
import type { HTMLAttributes, SvgComponent } from 'astro/types';
import type { IconName } from './icon';

export interface Props extends HTMLAttributes<"svg"> {
    name: IconName;
}
const { name, ...rest } = Astro.props;

if (!name) return ""

const Comp = (await import(`./${name}/index.svg`)).default as SvgComponent & ImageMetadata

---

<Comp {...rest} />
